<h4>All users</h4>
<%= will_paginate %>
<ul class="users">
  <% @users.each do |user| %>
      <div class="image_user_all">
        <%= image_tag user.image_url.to_s%>
      </div>
      <li>
        <%= user.email %> ,
        <%= link_to user.name, user %>
      </li>
      <% if current_user.admin? %>
          | <%= link_to "delete", user, :method => :delete, :confirm => "You sure?",
                        :title => "Delete #{user.name}" %>
      <% end %>
  <% end %>
</ul>
<%= will_paginate %>